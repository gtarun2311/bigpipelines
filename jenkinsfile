@Library('sharedlibrary_cicd')_
pipeline
{
    agent any
    stages
    {
        stage('Lcont download from github')
        {
            steps
            {
                script
                {
                    cicd1.download("maven")
                }
            }
        }
        stage('Lcont build')
        {
            steps
            {
                script
                {
                    cicd1.build()
                }
            }
        }
        stage('Lcont deployment')
        {
            steps
            {
                script
                {
                    cicd1.deployment("declarativepipelinewithsharedlibrary","172.31.82.37","testapp")
                }
            }
  }

  }
  }

